{
    "name": "seedly-frontend",
    "version": "1.0.0",
    "scripts": {
      "client": "cd client && yarn start",
      "dev-install": "yarn install && cd client && yarn install",
      "server": "nodemon server.js",
      "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
    },
    "dependencies": {
      "body-parser": "^1.18.3",
      "express": "^4.16.4"
    },
    "devDependencies": {
      "concurrently": "^4.0.1"
    }
  }